<Window x:Class="EeldataMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EeldataMonitor"
        mc:Ignorable="d"
        Title="EelData Monitor" Height="350" Width="525" ResizeMode="NoResize">

    <Grid>
        <DataGrid x:Name="dataGrid_SensorDatas" Loaded="dataGrid_SensorDatas_Loaded" HorizontalAlignment="Left" Height="321" Margin="0,0,0,-0.2" VerticalAlignment="Top" Width="354"/>
        <StackPanel HorizontalAlignment="Left" Height="321" Margin="354,0,0,-0.2" VerticalAlignment="Top" Width="164">
            <Button x:Name="btn_Feed" Click="btn_Feed_Click" Content="Feed"/>
            <Button x:Name="btn_CreateBassin" Click="btn_CreateBassin_Click" Content="Create Bassin"/>
        </StackPanel>
    </Grid>
</Window>
